  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "ai-plateau",
      "url": "/template/%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/template-import",
      "accept": {
        "text/markdown": [".md"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "scope_extensions": [
    {
      "origin": "https://claude.ai"
    }
  ],
  "related_applications": [
    {
      "platform": "chrome_web_store",
      "url": "https://chrome.google.com/webstore/detail/beyond-ai-plateau"
    }
  ],
  "prefer_related_applications": false
